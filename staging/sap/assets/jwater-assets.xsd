                    <xsd:documentation>Useful Life: Number Of Periods</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                        <xsd:totalDigits value="3"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
